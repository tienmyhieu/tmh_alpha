[
  { "type": "title", "lang": "zh", "translation": "0r0jnf8n", "identifier": "" },
  { "type": "topic", "lang": "", "translation": "lvjug92c", "identifier": "" },
  {
    "type": "vertical_quote_list",
    "lang": "zh",
    "translation": "ffhq0gea",
    "items": [
      { "active": "1", "lang": "zh", "page": "", "value": "參制之法雖照與部議不無差異而鑄成錢交呈式" },
      { "active": "1", "lang": "zh", "page": "", "value": "亦覺棈好准依此行〇十九年" },
      { "active": "1", "lang": "zh", "page": "", "value": "旨此次開設場局本為便民前者鑄以一初建設承辨" },
      { "active": "1", "lang": "zh", "page": "", "value": "拮据故不忍嚴為程督茲該等應工漸就嫺熟著再" },
      { "active": "1", "lang": "zh", "page": "", "value": "行依例鼔鑄俾雇借之民有所資仰〇十八年" },
      { "active": "1", "lang": "zh", "page": "", "value": "諭美號大銅錢前經鑄造頗覺無多茲著戸工二部都" },
      { "active": "1", "lang": "zh", "page": "", "value": "察侍衞派屬一人會同督工員據武庫支出好項銅" },
      { "active": "1", "lang": "zh", "page": "", "value": "錫鉛依法搭配煮鍊仍依年前字號規式鑄造一十" },
      { "active": "1", "lang": "zh", "page": "", "value": "萬文謹按美字各號內八字二十三號" },
      { "active": "1", "lang": "zh", "page": "", "value": "君君臣臣父父子子    賢賢親親樂樂利利" },
      { "active": "1", "lang": "zh", "page": "", "value": "國泰民安風調雨順    華封三祝天保九如" },
      { "active": "1", "lang": "zh", "page": "", "value": "得位得祿得名得壽    六府孔修三事允治" },
      { "active": "1", "lang": "zh", "page": "", "value": "欽定大南會典事例 戸部錢法 鼓鑄卷五十三 七" },
      { "active": "1", "lang": "zh", "page": "", "value": "至公至正無党無偏    萬歲萬歲萬萬歲壽" },
      { "active": "1", "lang": "zh", "page": "", "value": "穆穆皇皇擠擠蹌蹌        河流順軌年穀豐登" },
      { "active": "1", "lang": "zh", "page": "", "value": "親親長長老老幼幼        國富兵强內安外靜" },
      { "active": "1", "lang": "zh", "page": "", "value": "如山如川如岡如阜        福如東海壽比南山" },
      { "active": "1", "lang": "zh", "page": "", "value": "萬壽攸酢萬福攸同        天不愛道地不愛寶" },
      { "active": "1", "lang": "zh", "page": "", "value": "追琢其章金玉其相        王道蕩蕩聖謨洋洋" },
      { "active": "1", "lang": "zh", "page": "", "value": "一人有慶萬壽無疆        五辰順撫庶績其凝" },
      { "active": "1", "lang": "zh", "page": "", "value": "自天祐之吉無不利        四海共之萬世傳之" },
      { "active": "1", "lang": "zh", "page": "", "value": "澤及當時恩垂萬世        內四字十七號" },
      { "active": "1", "lang": "zh", "page": "", "value": "元亨利貞    福履綏將    帝德廣運    利用厚生" },
      { "active": "1", "lang": "zh", "page": "", "value": "中和位育    斂福錫民    四方爲則    剛健中正" },
      { "active": "1", "lang": "zh", "page": "", "value": "家給人足    川至山增    天下大同    萬世永賴" },
      { "active": "1", "lang": "zh", "page": "", "value": "萬物資生    解慍阜財    悠久無疆" },
      { "active": "1", "lang": "zh", "page": "", "value": "壽考萬年    裕國利民    〇二十年" },
      { "active": "1", "lang": "zh", "page": "", "value": "諭古號銅錢日久儲貯不免轉成頑弊曷若倂行攺鑄" },
      { "active": "1", "lang": "zh", "page": "", "value": "因念在京工匠數少不妨于河內原鑄場局再設鑪" },
      { "active": "1", "lang": "zh", "page": "", "value": "場不惟工匠人多便於趨事而小民得以傭工爲食" }
    ]
  },
